{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"declaration": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react",
		"lib": [
			"es5",
			"es2015",
			"es2016",
			"dom",
			"esnext"
		],
		"module": "es2015",
		"moduleResolution": "node",
		"noEmit": true,
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"outDir": "./dist/esm",
		"rootDir": "./src",
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"target": "ES6",
		"types": [
			"node"
		]
	},
	"include": [
		"./src/tline"
	]
}
